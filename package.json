{
	"name": "svelte-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"init:dev": "npx concurrently \"npx shx cp -n .env.example .env\" \"npx shx cp -n .env.example .env.test\" \"npx shx cp -n ./.vscode/default.settings.json ./.vscode/settings.json\" && npm run docker-up:dev && npm run migrate:init && npm run prisma:seed",
		"dev": "concurrently \"vite dev\" typesafe-i18n",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:unit && npm run test:e2e",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
		"test:e2e": "npm run docker-down:dev && npm run docker-down:test && npm run docker-up:test && npm run migrate:init && cross-env NODE_ENV=test prisma db seed && npx playwright install && cross-env NODE_ENV=test playwright test && npm run docker-down:test",
		"test:e2e:debug": "npm run docker-down:dev && npm run docker-down:test && npm run docker-up:test && npm run migrate:init && cross-env NODE_ENV=test prisma db seed && npx playwright install && cross-env NODE_ENV=test playwright test --debug && npm run docker-down:test",
		"lint": "eslint --ext .js,.cjs,.ts,.svelte .",
		"typecheck": "npm run check",
		"format": "prettier --plugin-search-dir . --check .",
		"lint:fix": "eslint --ext .js,.cjs,.ts,.svelte . --fix",
		"format:fix": "prettier --plugin-search-dir . --write .",
		"migrate:init": "prisma migrate dev --name init",
		"prepare-docker:dev": "shx mkdir -p ./docker-data/db && mkdir -p ./docker-data/redis && sudo chmod -R 777 ./docker-data && sudo chown -R 1001:1001 ./docker-data",
		"create-db-script": "shx rm -f ./db-config/init-database.sql && dotenv -e ./.env envsubst <./db-config/init-database.sql.txt >./db-config/init-database.sql",
		"config-keycloak": "dotenv -e ./.env ./keycloak/scripts/configure-keycloak.sh",
		"docker-up:test": "npm run create-db-script && docker compose -f docker-compose.test.yml up -d --wait --force-recreate --remove-orphans && npm run config-keycloak",
		"docker-up:dev": "npm run create-db-script && npm run prepare-docker:dev && docker compose -f docker-compose.dev.yml up -d --wait --remove-orphans && npm run config-keycloak",
		"docker-down:test": "docker compose -f docker-compose.test.yml down",
		"docker-down:dev": "docker compose -f docker-compose.dev.yml down",
		"docker-cleanup": "docker image prune && docker system prune -a",
		"remove-docker-volumes": "sudo rm -rf ./docker-data",
		"prisma:generate": "npx prisma generate",
		"prisma:migrate": "npx prisma migrate dev --name",
		"prisma:reset": "npx prisma migrate reset",
		"prisma:seed": "npx prisma db seed",
		"prisma:db:push": "npx prisma db push",
		"prisma:studio": "npx prisma studio",
		"upgrade-packages": "npx npm-upgrade",
		"coverage:report": "shx rm -rf coverage && shx rm -rf .nyc_output && npm run test:e2e && npx nyc report && npx http-server ./coverage -p 8090 -o"
	},
	"prisma": {
		"seed": "tsx ./prisma/seeds/index.ts"
	},
	"devDependencies": {
		"@auth/core": "^0.7.1",
		"@auth/sveltekit": "^0.3.1",
		"@floating-ui/dom": "^1.2.9",
		"@googleapis/youtube": "^9.0.0",
		"@next-auth/prisma-adapter": "^1.0.6",
		"@playwright/test": "^1.34.3",
		"@prisma/client": "^4.15.0",
		"@skeletonlabs/skeleton": "^1.6.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/kit": "^1.20.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@types/luxon": "^3.3.0",
		"@types/pino": "^7.0.5",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.0.1",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.2.1",
		"env-cmd": "^10.1.0",
		"eslint": "^8.41.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-svelte": "^2.29.0",
		"http-server": "^14.1.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"luxon": "^3.3.0",
		"next-auth": "^4.22.1",
		"npm-upgrade": "^3.1.0",
		"nyc": "^15.1.0",
		"pino": "^8.14.1",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"prisma": "^4.15.0",
		"prisma-erd-generator-markdown": "^1.3.1",
		"redis": "^4.6.6",
		"shx": "^0.3.4",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.2",
		"tsx": "^3.12.7",
		"typesafe-i18n": "^5.24.3",
		"typescript": "^5.0.4",
		"typescript-svelte-plugin": "^0.3.26",
		"vite": "^4.3.9",
		"vite-plugin-istanbul": "^4.1.0",
		"vitest": "^0.31.2",
		"zod": "^3.21.4"
	},
